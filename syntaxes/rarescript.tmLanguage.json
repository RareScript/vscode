{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "RareScript",
	"patterns": [{
		"name": "comment.line.percentage.rarescript",
		"match": "%%.*$"
	}, {
		"name": "keyword.control.rarescript",
		"match": "\\b(import|as|scope|return|cond|else|and|or|final|while|break|continue)\\b"
	}, {
		"name": "constant.language.boolean.rarescript",
		"match": "\\b(false|true|maybe)\\b"
	}, {
		"name": "string.quoted.double.rarescript",
		"begin": "\"",
		"end": "\"",
		"patterns": [{
			"name": "constant.character.escape.rarescript",
			"match": "\\\\."
		}]
	}, {
		"name": "constant.language.type.rarescript",
		"match": "#[^!@#$%^&*-+\\|/={}()<>0-9][^!@#$%^&*-+\\|/={}()<>]*::[^!@#$%^&*-+\\|/={}()<>0-9][^!@#$%^&*-+\\|/={}()<>]*"
	}, {
		"name": "entity.name.class.rarescript",
		"match": "[^!@#$%^&*-+\\|/={}()<>0-9][^!@#$%^&*-+\\|/={}()<>]*(?=::)"
	}, {
		"name": "entity.name.function.rarescript",
		"match": "(?<=::)[^!@#$%^&*-+\\|/={}()<>0-9][^!@#$%^&*-+\\|/={}()<>]*"
	}, {
		"name": "constant.numeric.rarescript",
		"match": "\\-?\\d+(\\.\\d+)?"
	}, {
		"name": "constant.language.property.dynamic.rarescript",
		"match": "(?<=->)@"
	}],
	"repository": {},
	"scopeName": "source.rare"
}